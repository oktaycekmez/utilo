<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="info">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t]%logger{36}%n    %d{HH:mm:ss.SSS} %-5level  - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>

        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>

    </Loggers>
</Configuration>